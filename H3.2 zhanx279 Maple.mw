rB:=&lt;1,1,0&gt;: rC:=&lt;-1,1,0&gt;: rD:=&lt;0,2,0&gt;: rE:=&lt;0,0.8,0&gt;:
rCD:=rD-rC: rCDHat:=1/Norm(rCD,2)*rCD:
rBD:=rD-rB: rBDHat:=1/Norm(rBD,2)*rBD:

ME:=&lt;MEx,MEy,MEz&gt;:

FC:=TCD*rCDHat:
FB:=TBD*rBDHat:
FE:=&lt;0,FBy,FBz&gt;:

ForceEqn:=FB+F1+F2+FC+FE:
TorqueEqn:= ME+ rE &amp;x FE + rB &amp;x FB + rC &amp;x FC + r1 &amp;x F1+ r2 &amp;x F2:
eqns:=[seq(ForceEqn[n],n=1..3), seq(TorqueEqn[n],n=1..3)];

solve(eqns);
